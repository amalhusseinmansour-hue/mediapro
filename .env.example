# ========================================
# ملف مثال لمتغيرات البيئة
# ========================================
#
# لاستخدام هذا الملف:
# 1. انسخ هذا الملف إلى .env
# 2. استبدل القيم الافتراضية بمفاتيحك الحقيقية
# 3. لا ترفع ملف .env إلى Git (موجود في .gitignore)
#
# ========================================

# ========== Social Media OAuth ==========

# Twitter/X API Keys
# احصل عليها من: https://developer.twitter.com/en/portal/dashboard
# الدليل الكامل: راجع دليل_تكوين_تويتر.md أو TWITTER_SETUP.md
TWITTER_API_KEY=
TWITTER_API_SECRET=

# Facebook App Credentials
# احصل عليها من: https://developers.facebook.com/apps
# ⚠️ مهم: ضع App ID أيضاً في: android/app/src/main/res/values/strings.xml
# الدليل الكامل: راجع دليل_تكوين_فيسبوك.md أو FACEBOOK_SETUP.md
# مثال: FACEBOOK_APP_ID=123456789012345
# مثال: FACEBOOK_APP_SECRET=abcd1234567890efgh
FACEBOOK_APP_ID=
FACEBOOK_APP_SECRET=

# LinkedIn API Keys
# احصل عليها من: https://www.linkedin.com/developers/apps
LINKEDIN_CLIENT_ID=
LINKEDIN_CLIENT_SECRET=

# TikTok API Keys
# احصل عليها من: https://developers.tiktok.com
TIKTOK_CLIENT_KEY=
TIKTOK_CLIENT_SECRET=

# ========== AI Services ==========

# OpenAI API Key (ChatGPT)
# احصل عليه من: https://platform.openai.com
OPENAI_API_KEY=

# Google Gemini API Key
# احصل عليه من: https://ai.google.dev
GEMINI_API_KEY=

# ========== Image Generation ==========

# Nano Banana API
# احصل عليه من: https://nanobanana.ai
NANO_BANANA_API_KEY=

# Stability AI API
# احصل عليه من: https://platform.stability.ai
STABILITY_AI_API_KEY=

# ========== Video Generation ==========

# Google Veo API
# احصل عليه من: https://ai.google.dev
GOOGLE_VEO_API_KEY=

# D-ID API
# احصل عليه من: https://www.d-id.com
DID_API_KEY=

# Synthesia API
# احصل عليه من: https://www.synthesia.io
SYNTHESIA_API_KEY=

# ========== Payment Gateway ==========

# Paymob Payment Gateway
# احصل عليها من: https://accept.paymob.com/portal2/en/settings
PAYMOB_API_KEY=
PAYMOB_PUBLIC_KEY=
PAYMOB_SECRET_KEY=
PAYMOB_HMAC_SECRET=
PAYMOB_INTEGRATION_ID=81249
PAYMOB_IFRAME_ID=81249

# تفعيل الوضع التجريبي (true/false)
PAYMOB_TEST_MODE=true

# العملة الافتراضية (SAR, AED, EGP, USD)
DEFAULT_CURRENCY=SAR
