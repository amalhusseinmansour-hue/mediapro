      },
      "name": "Generate Video",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 3,
      "position": [850, 400]
    },
    {
      "parameters": {
        "functionCode": "// Format the response\nconst response = $json;\nconst type = $node[\"Process Request\"].json.type;\nconst prompt = $node[\"Process Request\"].json.prompt;\n\nif (response.data) {\n  return {\n    json: {\n      success: true,\n      type: type,\n      prompt: prompt,\n      content_url: type === 'image' \n        ? (response.data.images ? response.data.images[0] : null)\n        : response.data.video_url,\n      credits_used: response.data.credits_used || 0,\n      generated_at: new Date().toISOString()\n    }\n  };\n} else {\n  return {\n    json: {\n      success: false,\n      message: 'Failed to generate content',\n      error: response.message || 'Unknown error'\n    }\n  };\n}"
      },
      "name": "Format Response",
      "type": "n8n-nodes-base.function",
      "typeVersion": 1,
      "position": [1050, 300]
    },
