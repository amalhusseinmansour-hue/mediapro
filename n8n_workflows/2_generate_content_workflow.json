        "options": {}
      },
      "name": "Webhook",
      "type": "n8n-nodes-base.webhook",
      "typeVersion": 1,
      "position": [250, 300],
      "webhookId": "generate-content"
    },
    {
      "parameters": {
        "functionCode": "// Extract data from webhook\nconst prompt = $json.body.prompt || '';\nconst type = $json.body.type || 'image'; // 'image' or 'video'\nconst options = $json.body.options || {};\nconst userId = $json.body.user_id || null;\n\nreturn {\n  json: {\n    prompt,\n    type,\n    options,\n    user_id: userId,\n    timestamp: new Date().toISOString()\n  }\n};"
      },
      "name": "Process Request",
      "type": "n8n-nodes-base.function",
      "typeVersion": 1,
      "position": [450, 300]
    },
    {
      "parameters": {
        "conditions": {
          "string": [
            {
              "value1": "={{$json.type}}",
              "operation": "equals",
              "value2": "image"
            }
          ]
        }
      },
      "name": "Check Type",
      "type": "n8n-nodes-base.if",
      "typeVersion": 1,
      "position": [650, 300]
    },
